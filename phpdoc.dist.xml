<?xml version="1.0" encoding="UTF-8" ?>
<phpdoc>
  <files>
    <directory>./src/lib</directory>
    <directory>./src/vendors/phine</directory>
    <ignore>*/test/*</ignore>
    <ignore>*/tests/*</ignore>
  </files>
  <parser>
    <target>docs/cache</target>
  </parser>
  <title>Phar Replace API</title>
  <transformations>
    <template name="clean"/>
  </transformations>
  <transformer>
    <target>docs</target>
  </transformer>
</phpdoc>
